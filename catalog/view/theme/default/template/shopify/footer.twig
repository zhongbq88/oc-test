    <div class="container">
      <div class="row">
        <div class="span12"> {{ powered }}</div>
        <!-- /span12 --> 
      </div>
      <!-- /row --> 
    </div>
    <!-- /container --> 
  </div>
  <!-- /footer-inner --> 
</div>
<!-- /footer --> 
 
</footer>
